    "director": "^1.2.8",
    "dom-autoscroller": "^2.3.1 ",
    "dragula": "^3.7.2",
    "flatpickr": "^4.5.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
