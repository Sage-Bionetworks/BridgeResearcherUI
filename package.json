  "scripts": {
    "clear": "rm -rf app/dist/",
    "clear:all": "rm -rf node_modules/ && npm run clear",
    "count": "find app/src -name '*.js' | xargs wc -l | sort -r",
    "release": "webpack --config webpack/webpack.config.prod.js",
    "dev": "webpack-dev-server --contentBase app --config webpack/webpack.config.dev.js",
    "test": "./node_modules/.bin/mocha --require @babel/register"
  },
